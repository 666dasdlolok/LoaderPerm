	LoadingTitle = "Verny Hub Loader",
	LoadingSubtitle = "by VernyFX",
	ConfigurationSaving = {
		Enabled = true,
		FolderName = nil, -- Create a custom folder for your hub/game
		FileName = "Big Hub"
	},
        Discord = {
        	Enabled = true,
        	Invite = "dwuSS95ecF", -- The Discord invite code, do not include discord.gg/
